@model DegreeMapping.Models.Course
@{
    int catalogId = Model.CatalogId;
    ViewBag.Title = Model.Degree;
    DegreeMapping.Models.Catalog cy = DegreeMapping.Models.Catalog.Get(Model.CatalogId);

}

@section JumbotronMain {
    <div class="jumbotron jumbotron-fluid" style="background-color:@cy.BGColor">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="h3">@Html.ActionLink("Degree View", "DegreeView", "App", new { id = Model.DegreeId }, null)</div>
                    <h1 class="heading-underline">@Model.CatalogYear @Model.Degree</h1>
                    <h2>@Model.Institution</h2>
                </div>
            </div>
        </div>
    </div>
}

<div class="container px-5">
    @Html.Action("_Course", "App", new { model = Model })
</div>
