@model DegreeMapping.Models.Course
@{
    int catalogId = Model.CatalogId;
    ViewBag.Title = Model.Degree;
}

@section JumbotronMain {
    <div class="jumbotron jumbotron-fluid">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="h3">@Html.ActionLink("Degree List", "DegreeList", "App",new { id = Model.CatalogId }, null) <span class="glyphicon glyphicon-arrow-right"></span> @Html.ActionLink(Model.Degree, "DegreeView", new { id = Model.DegreeId })</div>
                    <h1 class="heading-underline">@Model.CatalogYear @Model.Degree</h1>
                    <h2>@Model.Institution</h2>
                </div>
            </div>
        </div>
    </div>
}

<div class="container px-5">
    @Html.Action("_Course", "App", new { model = Model })
</div>
