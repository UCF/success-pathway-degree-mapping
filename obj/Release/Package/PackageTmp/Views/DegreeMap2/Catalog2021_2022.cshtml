@{
    Layout = "~/Views/Shared/_Pathway.cshtml";
}

<div class="container pt-5">
    <div class="row">
        <div class=" col-md-12">
            <div class="ucf-section ucf-section-degree-alpha-list ">
                <div class="h4 text-uppercase text-center">Search by Major</div>
                <p>
                    <div class="d-flex" id="degree-alpha-list">
                        <ul class="h4 list-inline mx-auto justify-content-center">
                            <li style="display: table-cell;"><a class="pr-2" href="#A">A</a></li>
                            <li style="display: table-cell;"><a class="pr-2" href="#B">B</a></li>
                            <li style="display: table-cell;"><a class="pr-2" href="#C">C</a></li>
                            <li style="display: table-cell;"><a class="pr-2" href="#D">D</a></li>
                            <li style="display: table-cell;"><a class="pr-2" href="#E">E</a></li>
                            <li style="display: table-cell;"><a class="pr-2" href="#F">F</a></li>
                            <li style="display: table-cell;"><a class="pr-2" href="#H">H</a></li>
                            <li style="display: table-cell;"><a class="pr-2" href="#I">I</a></li>
                            <li style="display: table-cell;"><a class="pr-2" href="#J">J</a></li>
                            <li style="display: table-cell;"><a class="pr-2" href="#K">K</a></li>
                            <li style="display: table-cell;"><a class="pr-2" href="#L">L</a></li>
                            <li style="display: table-cell;"><a class="pr-2" href="#M">M</a></li>
                            <li style="display: table-cell;"><a class="pr-2" href="#N">N</a></li>
                            <li style="display: table-cell;"><a class="pr-2" href="#P">P</a></li>
                            <li style="display: table-cell;"><a class="pr-2" href="#R">R</a></li>
                            <li style="display: table-cell;"><a class="pr-2" href="#S">S</a></li>
                            <li style="display: table-cell;"><a class="pr-2" href="#T">T</a></li>
                            <li style="display: table-cell;"><a class="pr-2" href="#W">W</a></li>
                        </ul>
                    </div>
                </p>
                <div class="text-center"><a href="/DegreeMap2/" title="View Past Degrees">View Past Degrees</a></div>
            </div>

            <div class="">
                <form name="degreeSearch" role="search" class="form-horizontal" onsubmit="return false;">
                    <div class="form-group">
                        <label class="sr-only" for="keyword">Search degrees</label><br>
                        <input type="text" placeholder="Search degrees" class="form-control" name="keyword" id="keyword" autocomplete="true" aria-autocomplete="list" aria-haspopup="false" autocapitalize="off" autocorrect="off" title="Degree Search" aria-label="Degree Search">
                    </div>
                    <p></p>
                </form>
            </div>

            <div id="DegreeListOutput" class="">
                <blockquote>loading...</blockquote>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript" src="~/Scripts/v2/degreelist.js"></script>
@Html.Action("_PathwayDegreeFooter")

<script type="text/javascript">
    $(document).ready(function () {
        $('h1.header-title').html('Pathway Catalog 2021-2022');
    })
</script>