@model DegreeMapping.Models.Degree
@{
    ViewBag.Title = Model.Name;
    DegreeMapping.Models.Catalog cy = DegreeMapping.Models.Catalog.Get(Model.CatalogId);
}

@section JumbotronMain {
    <div class="jumbotron jumbotron-fluid" style="background-color:@cy.BGColor">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="h3">@Html.ActionLink("Degree List", "DegreeList", "App", new { id = Model.CatalogId }, null) </div>
                    <h1>@Model.CatalogYear  @Html.Raw(Model.Name)</h1>
                    <h2>@Model.Institution</h2>
                </div>
            </div>
        </div>
    </div>
}

<div class="container px-5">
    @Html.Action("_Degree", "App", new { model = Model })
</div>