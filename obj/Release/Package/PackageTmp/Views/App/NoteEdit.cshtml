@model DegreeMapping.Models.Note
@{
    //DegreeMapping.Models.Catalog cy = DegreeMapping.Models.Catalog.Get(Model.CatalogId);
}

@section JumbotronMain {
    <div class="jumbotron jumbotron-fluid">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="h3">@Html.ActionLink(Model.Institution, "InstitutionView", new { id = Model.InstitutionId }) <span class="glyphicon glyphicon-arrow-right"></span> @Html.ActionLink(Model.Degree, "DegreeView", new { id = Model.DegreeId })</div>
                    <h1 class="heading-underline">@Model.CatalogYear @Model.Degree <br /></h1>
                    <div class="h2">@Model.Name</div>
                    <h2>@Model.Institution</h2>
                </div>
            </div>
        </div>
    </div>
}

<div class="row">
    <div class="col-md-3 alert alert-warning mx-0 my-0 h5">
        <small>@DegreeMapping.Models.Note.Message.Get(Model.Institution)</small>
    </div>
</div>

<div class="container pt-5">
    @Html.Action("_Note", "App", new { model = Model })
</div>