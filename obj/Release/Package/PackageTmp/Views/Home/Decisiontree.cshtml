@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Decisiontree</title>
    <script src="~/Content/Accessible-jQuery-Decision-Tree-Flowchart-Plugin-decisionTree/js/jquery-2.1.3.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    @*<script src="~/Content/Accessible-jQuery-Decision-Tree-Flowchart-Plugin-decisionTree/js/jquery.decisionTree.min.js"></script>*@
    <script src="~/Content/Accessible-jQuery-Decision-Tree-Flowchart-Plugin-decisionTree/js/jquery.decisionTree.js"></script>

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link href="~/Content/site-override.css" rel="stylesheet" />
    <link href="~/Content/Accessible-jQuery-Decision-Tree-Flowchart-Plugin-decisionTree/css/decisionTree.css" rel="stylesheet" />

</head>
<body>
    <div>
        @*https://www.jqueryscript.net/demo/Accessible-jQuery-Decision-Tree-Flowchart-Plugin-decisionTree/*@
        @*https://www.jqueryscript.net/chart-graph/Accessible-jQuery-Decision-Tree-Flowchart-Plugin-decisionTree.html*@
    </div>

    <div class="decisionTree" aria-live="polite" aria-relevant="additions">

        <p class="cta">
            <a href="#" role="button" data-start-node="start">
                Need help finding your path?
            </a>
        </p>

    </div>
    <script type="text/javascript">
        $(".decisionTree").decisionTree({

            // folder to locate JSON files, default is the same folder as the HTML page
            data_folder: '/pathway/Content/Accessible-jQuery-Decision-Tree-Flowchart-Plugin-decisionTree/',

            // prefix for question nodes
            node_prefix: 'dtn',

            // file extension for JSON requests
            json_ext: 'json',

            // cache JSON responses
            cache_json: false,

            // text for the 'Loading' overlay
            loading_text: 'Loading...',

            // location of the 'progress' animated GIF
            loading_gif: "/pathway/Content/Accessible-jQuery-Decision-Tree-Flowchart-Plugin-decisionTree/img/loading.gif",

            // speed of transitions and effects
            animation_speed: 600,

            // show additional copy on node questions
            question_extra_text: true,

            // hide the content used to introduce the decision tree
            hide_original_content: true,

            // make the headline of answer nodes a link
            answer_link: true,

            // target for the answer node link
            answer_target: "_blank"

        });
        //$(".decisionTree").decisionTree();
    </script>
</body>
</html>
